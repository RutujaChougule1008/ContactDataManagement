<configuration>
    <system.webServer>
        <handlers>
            <add name="Python FastCGI" path="*.py" verb="*" modules="FastCgiModule" scriptProcessor="C:\Users\lenovo\AppData\Local\Programs\Python\Python312\python.exe|C:\Users\lenovo\AppData\Local\Programs\Python\Python312\Lib\site-packages\wfastcgi.py" resourceType="Unspecified" requireAccess="Script" />
        </handlers>
        <rewrite>
            <rules>
                <rule name="Python API" stopProcessing="true">
                    <match url="^api/(.*)" />
                    <action type="Rewrite" url="run.py" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
